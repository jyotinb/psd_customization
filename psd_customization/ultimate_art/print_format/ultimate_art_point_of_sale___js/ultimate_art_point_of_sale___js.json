 "html": "<style>\n  @media screen {\n    .print-format {\n      height: 8.2in;\n      width: 5.8in;\n      padding: 0.25in;\n    }\n  }\n  .psd-pos {\n    font-family: Monospace;\n    line-height: 1.5;\n    font-size: 9pt;\n    display: flex;\n    flex-flow: column nowrap;\n  }\n  @media print {\n    .psd-pos {\n      min-height: 7.4in;\n    }\n  }\n  .psd-pos header {\n    padding-bottom: 1em;\n    margin-bottom: 1em;\n    border-bottom: 1px solid #d1d8dd;\n  }\n  .psd-pos footer {\n    padding-top: 1em;\n    margin-top: 1em;\n    border-top: 1px solid #d1d8dd;\n  }\n  .psd-pos header > h1 {\n    text-align: center;\n    text-transform: uppercase;\n    font-size: 1.5em;\n    font-weight: bold;\n    margin: 0;\n  }\n  .psd-pos > section:last-of-type {\n    flex: 1 1 auto;\n    display: flex;\n    flex-flow: column nowrap;\n    justify-content: flex-end;\n  }\n  .psd-pos table {\n    font-size: 0.9em;\n  }\n  .psd-pos tfoot > tr:not(:first-of-type) > td {\n    border: none;\n  }\n  .psd-pos tfoot > tr:last-of-type > td:last-of-type {\n    font-weight: bold;\n    font-size: 1.1em;\n  }\n  .psd-draft-indicator {\n    text-transform: uppercase;\n    line-height: 1em;\n    padding: 0.1em 0.3em;\n    font-size: 0.7em;\n    font-weight: normal;\n    background-color:  #d1d8dd;\n  }\n  .psd-details dl {\n    margin: 0;\n    margin-bottom: 0.5em;\n  }\n  .psd-details dl > dt {\n    font-weight: normal;\n  }\n  .psd-details dl > dd {\n    font-weight: bold;\n  }\n  .psd-details dl > dd:not(:first-of-type) {\n    font-weight: normal;\n    font-size: 0.9em;\n  }\n  .psd-stamp-area {\n    padding-top: 3em;\n    font-size: 0.9em;\n    font-style: italic;\n  }\n</style>\n<div class=\"psd-pos\">\n  <header>\n    <h1>{{ company }}</h1>\n  </header>\n  <section>\n    <div class=\"col-xs-6\">\n      <dl>\n        <dt>{{ __(\"Customer\") }}</dt>\n        <dd>{{ customer_name }}</dd>\n        {% if address_display %}\n        <dd>{{ address_display.replace(\"<br>\", \" \") }}</dd>\n        {% endif %}\n        {% if contact_mobile %}\n        <dd>{{ contact_mobile }}</dd>\n        {% endif %}\n      </dl>\n    </div>\n    <div class=\"col-xs-6\">\n      <dl>\n        <dt>{{ __(\"POS No\") }}</dt>\n        <dd>{{ offline_pos_name }}</dd>\n      </dl>\n      <dl>\n        <dt>{{ __(\"Date\") }}</dt>\n        <dd>{{ dateutil.global_date_format(posting_date) }}</dd>\n      </dl>\n    </div>\n  </section>\n  <table class=\"table table-condensed\">\n    <thead>\n      <tr>\n        <th width=\"40%\">{{ __(\"Item\") }}</b></th>\n        <th width=\"23%\" class=\"text-right\">{{ __(\"Qty\") }}</th>\n        <th width=\"17%\" class=\"text-right\">{{ __(\"Discount\") }}</th>\n        <th width=\"20%\" class=\"text-right\">{{ __(\"Amount\") }}</th>\n      </tr>\n    </thead>\n    <tbody>\n      {% for item in items %}\n      <tr>\n        <td>\n          {{ item.item_name }}\n        </td>\n        <td class=\"text-right\">\n          {{ format_number(item.qty, null, precision(\"difference\")) }}\n          {{ item.stock_uom }}<br/>\n          @ {{ format_currency(item.price_list_rate, currency) }}\n        </td>\n        <td class=\"text-right\">\n          {% if item.discount_percentage %}\n          {{ format_currency(item.qty * item.price_list_rate * item.discount_percentage / 100, currency) }}\n          {% else %} - {% endif %}\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(item.amount, currency) }}\n        </td>\n      </tr>\n      {% endfor %}\n    </tbody>\n    <tfoot>\n      {% if discount_amount %}\n      <tr>\n        <td class=\"text-right\" colspan=\"3\">\n          {{ __(\"Total\") }}\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(total, currency) }}\n        </td>\n      </tr>\n      {% endif %}\n      {% for row in taxes %}\n      {% if not row.included_in_print_rate %}\n      <tr>\n        <td class=\"text-right\" colspan=\"3\">\n          {{ row.description }}\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(row.tax_amount, currency) }}\n        </td>\n      </tr>\n      {% endif %}\n      {% endfor %}\n      {% if discount_amount %}\n      <tr>\n        <td class=\"text-right\" colspan=\"3\">\n          {{ __(\"Discount\") }}\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(discount_amount, currency) }}\n        </td>\n      </tr>\n      {% endif %}\n      <tr>\n        <td class=\"text-right\" colspan=\"3\">\n          {{ __(\"Grand Total\") }}\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(grand_total, currency) }}\n        </td>\n      </tr>\n      {% if rounding_adjustment %}\n      <tr>\n        <td class=\"text-right\" colspan=\"3\">\n          {{ _(\"Round Off\") }} ({{ format_currency(rounding_adjustment, currency) }})\n        </td>\n        <td class=\"text-right\">\n          {{ format_currency(rounded_total, currency) }}\n        </td>\n      </tr>\n      {% endif %}\n    </tfoot>\n  </table>\n  <section>\n    <p class=\"text-right psd-stamp-area\">{{ _(\"Authorized Signatory\")}}</p>\n  </section>\n  <footer>\n    <p>{{ terms }}</p>\n    <p class=\"text-center\">{{ __(\"Thank you, please visit again.\") }}</p>\n  </footer>\n</div>\n", 
